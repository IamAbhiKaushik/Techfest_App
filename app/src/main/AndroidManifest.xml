<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techfest.tf2017.abcd">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- <uses-permission android:name="android.permission.GET_ACCOUNTS" /> -->
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.MY_PERMISSIONS_REQUEST_LOCATION" />

    <application
        android:name="com.techfest.tf2017.abcd.TechfestApp"
        android:allowBackup="true"
        android:icon="@drawable/new_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tf"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.techfest.tf2017.abcd.LoginActivity"
            android:screenOrientation="portrait" />

        <service android:name="com.techfest.tf2017.abcd.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.techfest.tf2017.abcd.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.techfest.tf2017.abcd.HomePage"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.techfest.tf2017.abcd.CompetetionActivity"
            android:parentActivityName="com.techfest.tf2017.abcd.HomePage"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.techfest.tf2017.abcd.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="com.techfest.tf2017.abcd.MapListActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>